      "maxLength": 60,
      "unique": true
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "Department": {
      "type": "enumeration",
      "enum": [
        "Construction",
        "MANDE",
        "FitOut",
        "NewRegions",
        "Logistics",
        "None"
      ]
    },
    "Sales": {
      "type": "biginteger"
    }
  }
}
